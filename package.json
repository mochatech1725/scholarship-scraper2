{
  "name": "scholarship-scraper2",
  "version": "1.0.0",
  "description": "AWS-based college scholarship scraper",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "cdk": "cdk",
    "deploy:dev": "cdk deploy --profile default -c environment=dev",
    "deploy:staging": "cdk deploy --profile default -c environment=staging",
    "deploy:prod": "cdk deploy --profile default -c environment=prod",
    "destroy:dev": "cdk destroy --profile default -c environment=dev",
    "destroy:staging": "cdk destroy --profile default -c environment=staging",
    "destroy:prod": "cdk destroy --profile default -c environment=prod",
    "diff:dev": "cdk diff -c environment=dev",
    "diff:staging": "cdk diff -c environment=staging",
    "diff:prod": "cdk diff -c environment=prod",
    "bootstrap": "cdk bootstrap -c environment=dev",
    "setup": "npm install && npm run bootstrap",
    "docker:build:dev": "./scripts/build-and-push-docker.sh dev",
    "docker:build:staging": "./scripts/build-and-push-docker.sh staging",
    "docker:build:prod": "./scripts/build-and-push-docker.sh prod",
    "test:careerone": "ts-node test-careerone-local.ts",
    "populate:websites:dev": "./scripts/run-populate-websites.sh",
    "populate:websites:staging": "ENVIRONMENT=staging ./scripts/run-populate-websites.sh",
    "populate:websites:prod": "ENVIRONMENT=prod ./scripts/run-populate-websites.sh",
    "cleanup:expired:dev": "ts-node scripts/cleanup-expired-scholarships.ts",
    "cleanup:expired:staging": "ENVIRONMENT=staging ts-node scripts/cleanup-expired-scholarships.ts",
    "cleanup:expired:prod": "ENVIRONMENT=prod ts-node scripts/cleanup-expired-scholarships.ts",
    "migrate:mysql:dev": "ts-node scripts/migrate-to-mysql.ts",
    "migrate:mysql:staging": "ENVIRONMENT=staging ts-node scripts/migrate-to-mysql.ts",
    "migrate:mysql:prod": "ENVIRONMENT=prod ts-node scripts/migrate-to-mysql.ts",
    "update:secret:dev": "ts-node scripts/update-secret-with-mysql.ts",
    "update:secret:staging": "ENVIRONMENT=staging ts-node scripts/update-secret-with-mysql.ts",
    "update:secret:prod": "ENVIRONMENT=prod ts-node scripts/update-secret-with-mysql.ts",
    "mysql:password:set:dev": "ts-node scripts/manage-mysql-password.ts set",
    "mysql:password:set:staging": "ENVIRONMENT=staging ts-node scripts/manage-mysql-password.ts set",
    "mysql:password:set:prod": "ENVIRONMENT=prod ts-node scripts/manage-mysql-password.ts set",
    "mysql:password:get:dev": "ts-node scripts/manage-mysql-password.ts get",
    "mysql:password:get:staging": "ENVIRONMENT=staging ts-node scripts/manage-mysql-password.ts get",
    "mysql:password:get:prod": "ENVIRONMENT=prod ts-node scripts/manage-mysql-password.ts get",
    "mysql:password:info:dev": "ts-node scripts/manage-mysql-password.ts info",
    "mysql:password:info:staging": "ENVIRONMENT=staging ts-node scripts/manage-mysql-password.ts info",
    "mysql:password:info:prod": "ENVIRONMENT=prod ts-node scripts/manage-mysql-password.ts info",
    "create:secret:dev": "ts-node scripts/create-secret.ts",
    "create:secret:staging": "ENVIRONMENT=staging ts-node scripts/create-secret.ts",
    "create:secret:prod": "ENVIRONMENT=prod ts-node scripts/create-secret.ts",
    "test:mysql:dev": "ts-node scripts/test-mysql-connection.ts",
    "test:mysql:staging": "ENVIRONMENT=staging ts-node scripts/test-mysql-connection.ts",
    "test:mysql:prod": "ENVIRONMENT=prod ts-node scripts/test-mysql-connection.ts",
    "test:mysql:batch:dev": "ts-node scripts/test-mysql-batch.ts",
    "test:mysql:batch:staging": "ENVIRONMENT=staging ts-node scripts/test-mysql-batch.ts",
    "test:mysql:batch:prod": "ENVIRONMENT=prod ts-node scripts/test-mysql-batch.ts"
  },
  "devDependencies": {
    "@types/jest": "^29.0.0",
    "@types/node": "^20.0.0",
    "@types/uuid": "^9.0.0",
    "aws-cdk": "^2.0.0",
    "aws-cdk-lib": "^2.0.0",
    "constructs": "^10.0.0",
    "jest": "^29.0.0",
    "ts-jest": "^29.0.0",
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "@aws-cdk/aws-lambda-nodejs": "^1.203.0",
    "@aws-sdk/client-batch": "^3.0.0",
    "@aws-sdk/client-bedrock-runtime": "^3.0.0",
    "@aws-sdk/client-dynamodb": "^3.0.0",
    "@aws-sdk/client-eventbridge": "^3.0.0",
    "@aws-sdk/client-lambda": "^3.0.0",
    "@aws-sdk/client-secrets-manager": "^3.0.0",
    "@aws-sdk/client-s3": "^3.0.0",
    "@aws-sdk/lib-dynamodb": "^3.0.0",
    "@aws-sdk/s3-request-presigner": "^3.0.0",
    "axios": "^1.0.0",
    "cheerio": "^1.0.0",
    "dotenv": "^16.0.0",
    "mysql2": "^3.0.0",
    "puppeteer": "^21.0.0",
    "uuid": "^9.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
